    //local effect eff
    //local location loc = Location( GetRectCenterX(rcStart), GetRectCenterY(rcTarget) )
    //set eff = AddSpecialEffectLoc( "Abilities\\Spells\\NightElf\\Blink\\BlinkCaster.mdl", loc)
